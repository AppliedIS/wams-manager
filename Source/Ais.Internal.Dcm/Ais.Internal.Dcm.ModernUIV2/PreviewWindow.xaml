<mui:ModernDialog x:Class="Ais.Internal.Dcm.ModernUIV2.PreviewWindow"
                  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                  xmlns:mui="http://firstfloorsoftware.com/ModernUI"
                  Title="Preview" SizeToContent="WidthAndHeight" 
                  WindowStartupLocation="CenterScreen"
                  WindowStyle="ToolWindow" ResizeMode="NoResize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="40"/>
        </Grid.RowDefinitions>
        <WebBrowser DockPanel.Dock="Top" x:Name="webBrowser" HorizontalAlignment="Left" Width="550"   VerticalAlignment="Top" Visibility="Visible"/>
    </Grid>
</mui:ModernDialog>
