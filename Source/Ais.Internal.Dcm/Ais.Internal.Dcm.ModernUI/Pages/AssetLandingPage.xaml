<Page x:Class="Ais.Internal.Dcm.ModernUI.Pages.AssetLandingPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
       xmlns:mui="http://firstfloorsoftware.com/ModernUI"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
	Title="AssetLandingPage">

    <Grid>
        <StackPanel Grid.Column="1">
            <mui:ModernTab Layout="List"  SelectedSource="../Tabs/AssetFiles.xaml">
                <mui:ModernTab.Links>
                    <mui:Link DisplayName="Files"  Source="../Tabs/AssetFiles.xaml"/>
                    <mui:Link DisplayName="Thumbnails" Source="../Tabs/Thumbnails.xaml"/>
                    <mui:Link DisplayName="Outputs" Source="../Tabs/Outputs.xaml"/>
                    <!--<mui:Link DisplayName="Preview" Source="../Tabs/Viewer.xaml"/>-->
                </mui:ModernTab.Links>
            </mui:ModernTab>
        </StackPanel>
    </Grid>
</Page>
